<h1>Jak sie masz</h1>
<p>What do you feel like tonight?</p>

<% @restaurants.each do |restaurant| %>
  <div id="restaurant-<%= restaurant.id %>">
    <%= "#{restaurant.name}" %>
    <%= link_to 'Show page', restaurant_path(restaurant) %>
  </div>
<% end %>

<div id="map"></div>
<script>
  var map = new GMaps({
    el: '#map',
    lat: -12.043333,
    lng: -77.028333
  });
</script>
